{
  "name": "backoffice-api",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "NODE_OPTIONS=--enable-source-maps tsx watch src/server.ts",
    "build": "tsup src/server.ts --format esm --dts false --sourcemap --out-dir dist",
    "start": "node dist/server.js",
    "lint": "echo "(lint placeholder)"",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:seed": "prisma db seed"
  },
  "dependencies": {
    "@backoffice/shared": "workspace:*",
    "@prisma/client": "^5.18.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "pino": "^9.3.2",
    "pino-http": "^10.3.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "prisma": "^5.18.0",
    "tsup": "^8.2.4",
    "tsx": "^4.17.0",
    "typescript": "^5.6.3"
  },
  "prisma": {
    "seed": "node --loader tsx prisma/seed.ts"
  }
}
